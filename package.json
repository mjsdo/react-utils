{
  "name": "react-utils",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:esm-declaration": "node ./scripts/createEsmDeclarations.js",
    "build": "nx affected -t build && yarn build:esm-declaration",
    "build:all": "nx run-many -t build && yarn build:esm-declaration",
    "version:packages": "changeset version",
    "release:packages": "changeset publish",
    "test": "nx affected -t test",
    "cy:open": "cypress open",
    "cy:component": "nx affected -t cy:component",
    "cy:e2e": "start-server-and-test \"yarn storybook --ci\" http://localhost:6006 \"nx affected -t cy:e2e\"",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "deploy:storybook": "rimraf storybook-static && yarn build:storybook && touch ./storybook-static/.nojekyll && gh-pages -d ./storybook-static -t true",
    "type": "tsc --noEmit --incremental --watch"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@mjsdo/eslint-plugin": "^0.0.5",
